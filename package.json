  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbo",
    "build": "tsx src/lib/db/migrate.ts && next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "lint": "next lint && biome lint --write --unsafe",
    "lint:fix": "next lint --fix && biome lint --write --unsafe",
    "format": "biome format --write",
    "preview": "next start",
    "clean": "rimraf .next dist coverage",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx src/lib/db/migrate.ts",
    "db:studio": "drizzle-kit studio",
    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit pull",
    "db:check": "drizzle-kit check",
    "db:up": "drizzle-kit up",
    "db:seed": "tsx lib/db/seed.ts",
    "test": "export PLAYWRIGHT=True && pnpm exec playwright test",
    "test:watch": "export PLAYWRIGHT=True && pnpm exec playwright test --watch",
    "test:coverage": "export PLAYWRIGHT=True && pnpm exec playwright test --coverage",
    "test:unit": "jest",
    "test:e2e": "playwright test",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "validate": "pnpm run lint && pnpm run type-check && pnpm run test",
    "ci": "pnpm run lint && pnpm run build && pnpm run test"
